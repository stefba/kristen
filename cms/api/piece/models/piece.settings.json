{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "pieces",
  "info": {
    "name": "Piece"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Date": {
      "type": "datetime",
      "required": true
    },
    "Images": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "Meta": {
      "type": "component",
      "repeatable": false,
      "component": "meta.meta",
      "required": true
    }
  }
}
