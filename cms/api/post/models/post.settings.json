{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "posts",
  "info": {
    "name": "Post"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "Images": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "Text": {
      "type": "richtext"
    }
  }
}
